<Application x:Class="WebSocketManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WebSocketManager"
             Startup="AppOnStartup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Controls/Styles/ExtTextBox.xaml"/>
                <ResourceDictionary Source="Resources.xaml"/>
                <ResourceDictionary Source="Controls/Styles/StartButton.xaml"/>
                <ResourceDictionary Source="Controls/Styles/StopButton.xaml"/>
                <ResourceDictionary Source="Controls/Styles/Expander.xaml"/>
                <ResourceDictionary Source="Controls/Styles/CheckBox.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
